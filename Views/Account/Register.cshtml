
@model messager.models.Register
@using (Html.BeginForm("Register", "Account", FormMethod.Post))
{
	@Html.ValidationSummary()
	<p>Podaj nazwe uzytkownika: @Html.TextBoxFor(x=> x.UserName);</p>
	<p>Podaj nazwe Adres Email: @Html.TextBoxFor(x=> x.EmailAdress);</p>
	<p>Podaj Haslo uzytkownika: @Html.PasswordFor(x=> x.Password);</p>
	<button type="submit">rejestruj</button>
}
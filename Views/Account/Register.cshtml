<head>
	<link href="~/css/style.css" rel="stylesheet" />
</head>
@model messager.models.Register
@using (Html.BeginForm("Register", "Account", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
	<div id="regiserContainer">
	@Html.ValidationSummary()
	<p>Podaj nazwe uzytkownika @Html.TextBoxFor(x=> x.UserName, new {@class = "inputlogin"})</p>
	<p>Podaj Email uzytkownika @Html.TextBoxFor(x=> x.EmailAdress, new {@class = "inputlogin"})</p>
	<p>Podaj Haslo uzytkownika @Html.PasswordFor(x=> x.Password, new {@class = "inputlogin"})</p>
	<p>
			dodaj zdjecia <input class="customfiles" type="file" id="customfiles" name="files" multiple />
					
			
	</p>
	<button id="ButtonRegister"type="submit">Zajerestuj</button>
	</div>
}